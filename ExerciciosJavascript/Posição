//POSIÇÃO
var num, pos;
num = parseInt(prompt("Insira um número interno"));
pos = parseInt(prompt("Insira a posição"));
  console.log(num);
  console.log(pos);
num = num/Math.pow(10,pos);
  console.log(num);
pos = num-parseInt(num);
  console.log(pos);
pos *= 10;
  console.log(pos);
  console.log(parseInt(pos));

var n, p;
n = parseInt(prompt("Insira um número interno"));
p = parseInt(prompt("Insira a posição"));
n /= Math.pow(10,p);
n -= parseInt(n);
p = parseInt(n*10);
console.log(p);
