<html>
  <head>
    <h1>EH US GURI PAE</h1>
    <h6>troxao</h6>
  </head>
  <body>
    <script type="text/javascript">
      var a,b,c,dt,x1,x2;
      //Insere os termos a, b e c
      a=parseInt(prompt("Valor de A"));
      b=parseInt(prompt("Valor de B"));
      c=parseInt(prompt("Valor de C"));
      //Calcula o delta
      dt=b**2-4*a*c;
      //Calcula as raízes, se reais
      if (dt>=0) {
        x1=(-b+Math.sqrt(dt))/(2*a);
        x2=(-b-Math.sqrt(dt))/(2*a);
        alert("x1= "+x1+" e "+"x2= " +x2);
      } else {
        alert("dt= "+dt+" Não existe raiz real de números negativos");
      }
    </script>
  </body>
</html>
